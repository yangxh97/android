<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.beautyalarm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="BeautyAlarm"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="cn.studyjams.s2.sj0224.beautyalarm.MainActivity"
            android:label="BeautyAlarm"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="cn.studyjams.s2.sj0224.beautyalarm.remind">
        </activity>

        <activity android:name="cn.studyjams.s2.sj0224.beautyalarm.rest">
        </activity>


        <receiver android:name="cn.studyjams.s2.sj0224.beautyalarm.AlarmReceiver">
        </receiver>

        <service android:name="cn.studyjams.s2.sj0224.beautyalarm.RingtonePlayingService"
            android:enabled="true">

        </service>

    </application>
    <uses-permission android:name="android.permission.VIBRATE" />

</manifest>